{
  "name": "crdt-collab-editor-backend",
  "version": "1.0.0",
  "description": "Backend server for CRDT collaborative editor",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "express": "^4.21.2",
    "socket.io": "^4.7.5",
    "redis": "^4.7.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "express-rate-limit": "^7.4.1",
    "joi": "^17.13.3",
    "uuid": "^10.0.0",
    "yjs": "^13.6.20",
    "y-websocket": "^1.5.4",
    "aws-sdk": "^2.1691.0",
    "node-cron": "^3.0.3",
    "winston": "^3.17.0",
    "prom-client": "^15.1.3",
    "dotenv": "^16.4.7",
    "morgan": "^1.10.1",
    "@prisma/client": "^6.1.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.9",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^10.0.0",
    "@types/jest": "^29.5.14",
    "@types/node-cron": "^3.0.11",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2",
    "socket.io-client": "^4.8.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "prisma": "^6.1.0",
    "@types/pg": "^8.11.10"
  }
